 <td>\n              <button class=\"waves-effect btn-small blue darken-1\"><i class=\"material-icons\">create</i></button>\n              <button class=\"waves-effect btn-small red darken-1\"><i class=\"material-icons\">delete_sweep</i></button>\n            </td>\n\n          </tr>\n\n        </tbody>\n      \n      </table>\n\n    </div>\n\n  </div>\n</template>\n\n<script>\nimport Elementos from './service/elementos.js'\n\n  export default {\n\n    data(){\n      return{\n        elemento:{\n          serial:'',\n          modelo:'',\n          vendor:''\n        },\n        elementos:[]\n      }\n\n    },\n\n\n    mounted(){\n      Elementos.listar().then(resposta =>{\n        console.log(resposta.data)\n        this.elementos = resposta.data\n      })\n    },\n\n    methods:{\n      salvar(){\n        alert(this.elemento.key)\n      }\n    }\n  }\n\n</script>\n\n<style>\n\n</style>\n"],"mappings":"AA6DA,OAAOA,SAAQ,MAAO,wBAAuB;AAE3C,eAAe;EAEbC,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,QAAQ,EAAC;QACPC,MAAM,EAAC,EAAE;QACTC,MAAM,EAAC,EAAE;QACTC,MAAM,EAAC;MACT,CAAC;MACDC,SAAS,EAAC;IACZ;EAEF,CAAC;EAGDC,OAAOA,CAAA,EAAE;IACPP,SAAS,CAACQ,MAAM,CAAC,CAAC,CAACC,IAAI,CAACC,QAAO,IAAI;MACjCC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACT,IAAI;MACzB,IAAI,CAACK,SAAQ,GAAII,QAAQ,CAACT,IAAG;IAC/B,CAAC;EACH,CAAC;EAEDY,OAAO,EAAC;IACNC,MAAMA,CAAA,EAAE;MACNC,KAAK,CAAC,IAAI,CAACb,QAAQ,CAACc,GAAG;IACzB;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}